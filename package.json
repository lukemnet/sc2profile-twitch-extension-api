{
  "name": "sc2pte-backend",
  "version": "1.1.1",
  "description": "StarCraft II Profile Twitch Extension (SC2PTE) - backend",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "clearTestCache": "jest --clearCache",
    "dev": "ts-node-dev --respawn --transpileOnly ./src/index.ts",
    "prod": "tsc && node ./dist/index.js",
    "build": "rm -rf dist && tsc",
    "watch": "tsc -w",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint --project tsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lukemnet/sc2pte-backend.git"
  },
  "keywords": [
    "starcraft2",
    "sc2",
    "twitch",
    "twitchtv",
    "extension",
    "api",
    "ebs",
    "backend"
  ],
  "author": "Łukasz Wójcik",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lukemnet/sc2pte-backend/issues"
  },
  "homepage": "https://github.com/lukemnet/sc2pte-backend#readme",
  "dependencies": {
    "compression": "1.7.3",
    "cors": "2.8.5",
    "dotenv": "6.2.0",
    "fastify": "2.0.1",
    "fastify-blipp": "1.2.1",
    "fastify-plugin": "1.5.0",
    "mongoose": "5.4.17",
    "typescript": "3.3.3333"
  },
  "devDependencies": {
    "@types/compression": "0.0.36",
    "@types/cors": "^2.8.4",
    "@types/express": "4.16.1",
    "@types/jest": "24.0.9",
    "@types/mongoose": "5.3.20",
    "@types/node": "11.10.4",
    "@types/source-map-support": "0.5.0",
    "jest": "24.1.0",
    "prettier": "1.16.4",
    "source-map-support": "0.5.10",
    "ts-jest": "24.0.0",
    "ts-node-dev": "1.0.0-pre.32",
    "tslint": "5.13.1",
    "tslint-config-airbnb": "5.11.1",
    "tslint-config-prettier": "1.18.0",
    "tslint-eslint-rules": "5.4.0",
    "tslint-immutable": "5.3.0",
    "tslint-plugin-prettier": "2.0.1"
  }
}

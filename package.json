{
  "name": "sc2pte-backend",
  "version": "2.0.0",
  "private": true,
  "description": "StarCraft II Profile Twitch extension - backend service (EBS)",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist/*",
    "build": "npm run lint && npm run test && npm run clean && tsc",
    "lint": "tslint --project tsconfig.json",
    "runsas": "npm --prefix ../sc2-api-service run dev",
    "watch": "tsc-watch --onSuccess \"npm start\"",
    "dev": "parallel runsas watch | pino-pretty",
    "test": "jest",
    "clearTestCache": "jest --clearCache",
    "start": "node scripts/start.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sc2pte/backend.git"
  },
  "keywords": [
    "starcraft",
    "starcraft2",
    "community",
    "api",
    "player",
    "backend"
  ],
  "author": "Łukasz Wójcik",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sc2pte/backend/issues"
  },
  "homepage": "https://github.com/sc2pte/backend#readme",
  "dependencies": {
    "cloudflare": "2.7.0",
    "fastify": "2.15.1",
    "fastify-caching": "5.0.0",
    "fastify-cors": "3.0.3",
    "fastify-env": "1.0.1",
    "fastify-ioredis": "0.1.1",
    "fastify-log": "1.2.1",
    "fastify-plugin": "1.6.1",
    "fastify-redis": "4.0.1",
    "fastify-twitch-ebs-tools": "1.0.3",
    "mongoose": "5.9.26",
    "starcraft2-api": "1.3.8"
  },
  "devDependencies": {
    "@types/jest": "26.0.7",
    "@types/mongoose": "5.7.32",
    "@types/node": "14.0.26",
    "fastify-redis-mock": "1.0.0",
    "jest": "26.1.0",
    "npm-parallel": "2.0.1",
    "pino-pretty": "4.0.0",
    "rimraf": "3.0.2",
    "ts-jest": "26.1.4",
    "ts-mongoose": "0.0.21",
    "tsc-watch": "4.2.9",
    "tslint": "6.1.2",
    "tslint-config-airbnb": "5.11.2",
    "tslint-eslint-rules": "5.4.0",
    "tslint-immutable": "6.0.1",
    "typescript": "3.9.7"
  }
}

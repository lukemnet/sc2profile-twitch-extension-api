{
  "name": "sc2pte-backend",
  "version": "1.1.1",
  "description": "StarCraft II Profile Twitch Extension (SC2PTE) - backend",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "clearTestCache": "jest --clearCache",
    "dev": "ts-node-dev --respawn --transpileOnly ./src/index.ts",
    "prod": "tsc && node ./dist/index.js",
    "build": "rm -rf dist && tsc",
    "watch": "tsc -w",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint --project tsconfig.json",
    "proxy": "local-ssl-proxy --source 8884 --target 8883"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lukemnet/sc2pte-backend.git"
  },
  "keywords": [
    "starcraft2",
    "sc2",
    "twitch",
    "twitchtv",
    "extension",
    "api",
    "ebs",
    "backend"
  ],
  "author": "Łukasz Wójcik",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lukemnet/sc2pte-backend/issues"
  },
  "homepage": "https://github.com/lukemnet/sc2pte-backend#readme",
  "dependencies": {
    "fastify": "2.0.1",
    "fastify-auth": "0.4.0",
    "fastify-blipp": "1.2.1",
    "fastify-boom": "0.1.0",
    "fastify-cors": "2.1.2",
    "fastify-env": "0.6.2",
    "fastify-fast-helmet": "0.3.0",
    "fastify-graceful-shutdown": "2.0.0",
    "fastify-jwt": "0.9.3",
    "fastify-mongodb": "1.0.1",
    "fastify-plugin": "1.5.0",
    "fastify-rate-limit": "2.1.0"
  },
  "devDependencies": {
    "@types/jest": "24.0.9",
    "@types/node": "11.10.4",
    "jest": "24.1.0",
    "prettier": "1.16.4",
    "ts-jest": "24.0.0",
    "ts-node-dev": "1.0.0-pre.32",
    "tslint": "5.13.1",
    "tslint-config-airbnb": "5.11.1",
    "tslint-config-prettier": "1.18.0",
    "tslint-eslint-rules": "5.4.0",
    "tslint-immutable": "5.3.0",
    "tslint-plugin-prettier": "2.0.1",
    "typescript": "3.3.3333"
  }
}

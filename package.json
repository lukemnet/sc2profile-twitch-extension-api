{
  "name": "@sc2pte/panel-backend",
  "version": "2.0.0",
  "private": true,
  "description": "StarCraft II Profile Twitch extension - backend service (EBS)",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist/*",
    "compile": "tsc --project tsconfig.json",
    "build": "npm run lint && npm run test && npm run clean && npm run compile",
    "lint": "eslint . --ext .ts",
    "runsas": "npm --prefix ../sc2-api-service run dev",
    "watch": "tsc-watch --onSuccess \"npm start\"",
    "dev": "parallel runsas watch | pino-pretty",
    "test": "jest",
    "clearTestCache": "jest --clearCache",
    "start": "node scripts/start.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sc2pte/backend.git"
  },
  "keywords": [
    "starcraft",
    "starcraft2",
    "community",
    "api",
    "player",
    "backend"
  ],
  "author": "Łukasz Wójcik",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sc2pte/panel-backend/issues"
  },
  "homepage": "https://github.com/sc2pte/panel-backend#readme",
  "dependencies": {
    "@types/jest": "26.0.15",
    "@types/mongoose": "5.7.36",
    "@types/node": "14.14.3",
    "@typescript-eslint/eslint-plugin": "4.6.0",
    "@typescript-eslint/parser": "4.5.0",
    "cloudflare": "2.7.0",
    "eslint": "7.12.0",
    "eslint-config-airbnb-typescript": "12.0.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.1.0",
    "fastify": "3.7.0",
    "fastify-caching": "6.0.1",
    "fastify-cors": "4.1.0",
    "fastify-env": "2.1.0",
    "fastify-ioredis": "0.1.1",
    "fastify-log": "1.2.1",
    "fastify-plugin": "2.3.4",
    "fastify-redis": "4.0.3",
    "fastify-redis-mock": "1.0.0",
    "fastify-twitch-ebs-tools": "2.0.1",
    "mongoose": "5.10.11",
    "starcraft2-api": "1.5.0",
    "jest": "26.6.1",
    "npm-parallel": "2.0.1",
    "pino-pretty": "4.3.0",
    "rimraf": "3.0.2",
    "ts-jest": "26.4.2",
    "ts-mongoose": "0.0.21",
    "tsc-watch": "4.2.9",
    "typescript": "4.0.3"
  }
}
